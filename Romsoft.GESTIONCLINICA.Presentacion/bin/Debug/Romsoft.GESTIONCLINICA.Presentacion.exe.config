<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <add key="TimeOutSession" value="300" />
    <add key="CultureNameDefault" value="es-PE" />
    <add key="TimeZoneId" value="SA Pacific Standard Time" />
    <add key="TimeZoneGMT" value="-5" />

    <!--Color Borde Formulario-->
    <add key="ColorBodeControles" value="Color.RoyalBlue"/>
    <add key="ColorBodeFormularios" value="Pens.Gray"/>
    
    
    <!--Ruta Servicios-->
    <add key="WS_Account_Login" value="http://localhost:44390/api/Account/Login" />
    
    <!--WS USUARIO-->
    <add key="WS_Usuario_Add" value="http://localhost:44390/api/SEG_USUARIO/Add" />
    <add key="WS_Usuario_Delete" value="http://localhost:44390/api/SEG_USUARIO/Delete" />
    <add key="WS_Usuario_GetAllPaging" value="http://localhost:44390/api/SEG_USUARIO/GetAllPaging" />
    <add key="WS_Usuario_GetById" value="http://localhost:44390/api/SEG_USUARIO/GetById" />
    <add key="WS_Usuario_Update" value="http://localhost:44390/api/SEG_USUARIO/Update" />
    <add key="WS_Usuario_GetAllFilters" value="http://localhost:44390/api/SEG_USUARIO/GetAllFilters" />

    <!--WS TIPO_ESTADO-->
    <add key="WS_Estado_GetAllFilters" value="http://localhost:44390/api/TIPO_ESTADO/GetAllFilters" />

    <!--WS Ruta Rol-->
    <add key="WS_Rol_GetAllActives" value="http://localhost:44390/api/SEG_ROL/GetAllActives" />
    
  <!--WS Ocupacion-->
    <add key="WS_Ocupacion_Add" value="http://localhost:44390/api/ADM_OCUPACION/Add" />
    <add key="WS_Ocupacion_Delete" value="http://localhost:44390/api/ADM_OCUPACION/Delete" />
    <add key="WS_Ocupacion_GetById" value="http://localhost:44390/api/ADM_OCUPACION/GetById" />
    <add key="WS_Ocupacion_Update" value="http://localhost:44390/api/ADM_OCUPACION/Update" />
    <add key="WS_Ocupacion_GetAllFilters" value="http://localhost:44390/api/ADM_OCUPACION/GetAllFilters" />
    <add key="WS_Ocupacion_GetAllActives" value="http://localhost:44390/api/ADM_OCUPACION/GetAllActives" />
    
    <!--WS Tarifario Segus-->
    <add key="WS_CVN_TARIFARIO_SEGUS_Add" value="http://localhost:44390/api/CVN_TARIFARIO_SEGUS/Add" />
    <add key="WS_CVN_TARIFARIO_SEGUS_Delete" value="http://localhost:44390/api/CVN_TARIFARIO_SEGUS/Delete" />
    <add key="WS_CVN_TARIFARIO_SEGUS_Update" value="http://localhost:44390/api/CVN_TARIFARIO_SEGUS/Update" />
    <add key="WS_CVN_TARIFARIO_SEGUS_GetAllFilters" value="http://localhost:44390/api/CVN_TARIFARIO_SEGUS/GetAllFilters" />
    <add key="WS_CVN_TARIFARIO_SEGUS_GetAllActives" value="http://localhost:44390/api/CVN_TARIFARIO_SEGUS/GetAllActives" />
    <add key="WS_CVN_TARIFARIO_SEGUS_GetById" value="http://localhost:44390/api/CVN_TARIFARIO_SEGUS/GetById" />
    

    <!--clasificacion Segus-->
    <add key="WS_CVN_CLASIFICACION_SEGUS_GetAllActives" value="http://localhost:44390/api/CVN_CLASIFICACION_SEGUS/GetAllActives" />
  
    <!--Susalud-->
    <add key="WS_CVN_CLASIFICACION_SUSALUD_GetAllActives" value="http://localhost:44390/api/CVN_CLASIFICACION_SUSALUD/GetAllActives" />

    <!--Susalud_od-->
    <add key="WS_CVN_CLASIFICACION_SUSALUD_OD_GetAllActives" value="http://localhost:44390/api/CVN_CLASIFICACION_SUSALUD_OD/GetAllActives" />

    <!--Tipo Precio-->
    <add key="WS_CVN_TIPO_PRECIO_GetAllActives" value="http://localhost:44390/api/CVN_TIPO_PRECIO/GetAllActives" />

    <!--Centro Costo-->
    <add key="WS_CON_CENTRO_COSTO_GetAllActives" value="http://localhost:44390/api/CON_CENTRO_COSTO/GetAllActives" />
    
    <!--Cuenta Contable-->
    <add key="WS_CON_CUENTA_CONTABLE_GetAllActives" value="http://localhost:44390/api/CON_CUENTA_CONTABLE/GetAllActives" />

    <!--Detalle Participantes-->
    <add key="WS_CVN_TARIFARIO_SEGUS_PARTICIPANTE_Add" value="http://localhost:44390/api/CVN_TARIFARIO_SEGUS_PARTICIPANTE/Add" />
    <add key="WS_CVN_TARIFARIO_SEGUS_PARTICIPANTE_Delete" value="http://localhost:44390/api/CVN_TARIFARIO_SEGUS_PARTICIPANTE/Delete" />
     <add key="WS_CVN_TARIFARIO_SEGUS_PARTICIPANTE_GetAllActivesFilters" value="http://localhost:44390/api/CVN_TARIFARIO_SEGUS_PARTICIPANTE/GetAllActivesFilters" />
    
    <!--Categoria Pago-->
    <add key="WS_CVN_CATEGORIA_PAGO_GetAllActives" value="http://localhost:44390/api/CVN_CATEGORIA_PAGO/GetAllActives" />
    <add key="WS_CVN_CATEGORIA_PAGO_GetAllFilters" value="http://localhost:44390/api/CVN_CATEGORIA_PAGO/GetAllFilters" />
    <add key="WS_CVN_CATEGORIA_PAGO_Add" value="http://localhost:44390/api/CVN_CATEGORIA_PAGO/Add" />
    <add key="WS_CVN_CATEGORIA_PAGO_Delete" value="http://localhost:44390/api/CVN_CATEGORIA_PAGO/Delete" />
    <add key="WS_CVN_CATEGORIA_PAGO_GetById" value="http://localhost:44390/api/CVN_CATEGORIA_PAGO/GetById" />
    <add key="WS_CVN_CATEGORIA_PAGO_Update" value="http://localhost:44390/api/CVN_CATEGORIA_PAGO/Update" />


    <!--Detalle Segus Categoria Precio-->
    <add key="WS_CVN_CATEGORIA_PAGO_PRECIO_Add" value="http://localhost:44390/api/CVN_CATEGORIA_PAGO_PRECIO/Add" />
    <add key="WS_CVN_CATEGORIA_PAGO_PRECIO_Delete" value="http://localhost:44390/api/CVN_CATEGORIA_PAGO_PRECIO/Delete" />
    <add key="WS_CVN_CATEGORIA_PAGO_PRECIO_GetAllActivesFilters" value="http://localhost:44390/api/CVN_CATEGORIA_PAGO_PRECIO/GetAllActivesFilters" />

    

  </appSettings>

  <log4net>
    <appender name="DebugAppender" type="log4net.Appender.DebugAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="Log/logGestionClinica.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="100KB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="All" />
      <appender-ref ref="DebugAppender" />
      <appender-ref ref="RollingFileAppender" />
    </root>
  </log4net>
  
</configuration>